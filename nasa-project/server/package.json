{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start:development": "nodemon -r dotenv/config -r sucrase/register ./src/server.js",
    "start": "npm run build && npm run copy:kepler-data && npm run copy:public-folder && node -r dotenv/config dist/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "copy:kepler-data": "npx copyfiles -f ./src/data/*.csv ./dist/data",
    "copy:public-folder": "npx copyfiles -a ./public/** ./dist",
    "test": "jest --detectOpenHandles",
    "test-watch": "jest --watch --detectOpenHandles",
    "cluster": "npx pm2 start src/server.js -i max"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.2.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "sucrase": "^3.21.0",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.0.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.4.6",
    "morgan": "^1.10.0",
    "nasa-project": "file:.."
  }
}
